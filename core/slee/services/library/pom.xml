											org.mobicents.tools
										</groupId>
										<artifactId>
											maven-library-plugin
										</artifactId>
										<versionRange>
											[1.0.0.FINAL,)
										</versionRange>
										<goals>
											<goal>
												generate-descriptor
											</goal>
											<goal>
												copy-dependencies
											</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<ignore></ignore>
									</action>
								</pluginExecution>
							</pluginExecutions>
						</lifecycleMappingMetadata>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>

</project>

